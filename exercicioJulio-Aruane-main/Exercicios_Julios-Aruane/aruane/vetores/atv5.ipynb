    "# Exercicio 5\n",
    "\n",
    "lista = [1,2,3,4,5,6]\n",
    "number_busca = 3\n",
    "\n",
    "def exluir_elemento(lista, number_busca):\n",
    "  for indice, item in enumerate(lista):\n",
    "    if item == number_busca:\n",
    "      lista.pop(indice)\n",
    "      break\n",
    "\n",
    "exluir_elemento(lista, number_busca)\n",
    "print(lista)"
   ]
